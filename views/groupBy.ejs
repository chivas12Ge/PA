<div class="container">
    <div class="row">
      <div class="col-md-2" style="background-color: #f8f9fa; padding: 15px;">
        <p>amount</p>
      </div>
      <div class="col-md-2" style="background-color: #f8f9fa; padding: 15px;">
        <p>category</p>
      </div>
    </div>
    <% items.forEach(item=>{ %>
    <div class="row">
      <div class="col-md-2" style="background-color: #f8f9fa; padding: 15px;">
        <p><%=item.count %></p>
      </div>
      <div class="col-md-2" style="background-color: #f8f9fa; padding: 15px;">
        <p><%=item._id %></p>
      </div>
    </div>
    <% }) %>
  </div>
  